{{#each result}}
<!-- Modal Start-->
<div id="myModal1" class="modal fade" role="dialog">
 	 <div class="modal-dialog">

    <!-- Modal content-->
    		<div class="modal-content">
      			<div class="modal-header ">
      				<div class="container-fluid">
    					<div class="row">
     					   <div class="col-md-1"></div>
                
          				   <div class="col-md-10" >         
                           		<h4 class="modal-title ">{{this.name}}</h4>
                           </div>
        				   <div class="col-md-1"></div>
    					</div>
    				</div>
      								<button type="button" class="close" data-dismiss="modal">&times;</button>
      			</div>
      <form action="/cases/submitlawyer" method = "post"> 
      <div class="modal-body">
			<div class="container-fluid">
    			<div class="row">
      					<div class="col-md-1"></div>
                		<div class="col-md-10" >         
                        		<img src="img/400x400/l1.jpg" class="img-responsive" alt=".." width="350" height="350"   >
                        </div>
        				<div class="col-md-1"></div>
    			</div>

                <div class="row">
                			
                		<div class="col-md-12">         
                    
                			<div style="text-align: center">
                			<p>  </p>
                            <h6>&nbsp;&nbsp;Location: {{this.location}}</h6>
                            <h6>&nbsp;&nbsp;Speciality: {{this.specialization}}</h6>
                            <h6>&nbsp;&nbsp;E-mail ID:  {{this.email}}</h6>
                            <h6>&nbsp;&nbsp;Fees: &#8377;2,00,000</h6>
                            <h6>&nbsp;&nbsp;Rating :4.6</h6>
                            </div>
                           <br>
                             <h5 style="margin-left: 40px;">Case ID</h5>
                           
                            <div class="form-group col-md-4">
								<select name = "casename" class="form-control">
									{{#each ../mycase}}
										<option value="{{this.case_name}}">{{this.case_name}}</option>
									{{/each}}
								</select>
                            </div>

                            
                        
          				</div>      
                </div>         
      		</div>
       </div>
      

      			<div class="modal-footer">
      			<button type="submit" name = "lawyers_email" value = "{{this.email}}" class="btn btn-success">Submit Case </button>
      	  			<button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
      			</div>
    	</div>
		</form>

  	</div>
</div>
<!-- Modal End-->

                 <div class="col-md-3 col-sm-3 col-xs-6">
                      <div class="alert alert-info back-widget-set text-center">
                            
                             <button type="button"  data-toggle="modal" data-target="#myModal1">
                            <img src="/images/l2.jpg" alt="..." class="img-thumbnail" >
                            <h5>Name: {{this.name}} </h5>
                            <h5>Spec: {{this.specialization}} </h5>
                            <h5>Loc: {{this.location}} </h5>
                           </button>
                            
                        </div>
                    </div>
{{else}}
<strong>Sorry !</strong> No such lawyer exists.
{{/each}}
  
 
